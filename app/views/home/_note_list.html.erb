<div class="row">
	<h3>Notes</h3>
	<%= link_to '+ Add Note', new_note_path, :class => "button tiny right" %>
	<div class="small-12 medium-6 medium-centered columns">
		<% @notes.each do |note| %>
			<div class="small-12 columns">
				<strong><%= note.name %></strong>
			</div>
			<div class="small-12 columns">
				<%= note.desc %>
			</div>
		<% end %>
	</div>
</div>